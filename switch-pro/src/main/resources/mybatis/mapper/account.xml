<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AccountMapper">
	<insert id="insertAccount" parameterType="signVO">
		INSERT INTO OSCUSER VALUES(OSCUSER_SEQ.NEXTVAL, #{userId}, 
			#{userPw}, #{userName}, #{userEmail}, #{userPhone}, SYSDATE)
	</insert>
</mapper>